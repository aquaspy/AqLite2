{
  "name": "aqlite2",
  "version": "0.2.0",
  "description": "A simple fork of AqLite to add some small features",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [
    "aqw;",
    "aqlite2;"
  ],
  "author": "aquasp",
  "license": "MIT",
  "devDependencies": {
    "electron": "^4.2.12",
    "electron-builder": "^22.8.1",
    "electron-localshortcut": "^3.2.1",
    "nw-flash-trust": "^0.3.0"
  },
  "build": {
    "asar": false,
    "appId": "aquasp",
    "mac": {
      "category": "Games"
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ],
      "category": "Game"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "icon": "Icon/Icon.png"
  },
  "dependencies": {
    "electron-localshortcut": "^3.2.1",
    "nw-flash-trust": "^0.3.0"
  }
}
